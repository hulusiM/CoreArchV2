<div id="CompareFuelModal" class="modal fade" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header bg-orange-300">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h6 class="modal-title">Yakıt Bilgileri Karşılaştırma</h6>
            </div>
            <div class="modal-body" style="max-height: 80vh;overflow-y: auto;">
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-body has-bg-image">
                            <table class="table-striped table-hover table-bordered table datatable-basic table-loader"
                                   id="dataTableDebitFuelCompare"
                                   table-id="dataTable-dataTableDebitFuelCompare"
                                   table-model-url=""
                                   table-DatabaseName="/Maintenance"
                                   table-tool-type="dataTable">
                                <thead>
                                    <tr class="filters">
                                        <th style="text-align: center; width: 1%;">No</th>
                                        <th data-value="Plate" style="text-align: center;">
                                            Plaka
                                        </th>
                                        <th data-value="Amount" data-type="money" style="text-align: center;">
                                            Tutar
                                        </th>
                                        <th data-value="FuelDate" data-type="date" style="text-align: center;">
                                            Yakıt Tarihi
                                        </th>
                                        <th data-value="DebitDateRange" style="text-align: center;">
                                            Zimmet Tarihleri
                                        </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script>

    function fuelCompareDebit() {
        var model = { StartDate: $('#StartDate').val(), EndDate: $('#EndDate').val() }
        sendAjxForm(model,
            "/Report/VehicleDebitAndMonthlyFuelCompare",
            function (e) {
                if (e.length > 0) {
                    funcCustomDataTable(e, "dataTable-dataTableDebitFuelCompare");
                    $('#CompareFuelModal').modal("show");
                } else
                    ShowMessage("info", "Bilgi", "Karşılaştırılacak veri bulunamadı");
            }, "GET");
    }
</script>